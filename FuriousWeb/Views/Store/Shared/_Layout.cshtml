<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <title>@ViewBag.Title - ZeShop</title>
    @Styles.Render("~/Content/Styles/shop")
    <!--[if lt IE 9]>
    <script src="js/html5shiv.js"></script>
    <script src="js/respond.min.js"></script>
    <![endif]-->
</head><!--/head-->

<body>
    <div class="page">
        <header id="header">
            <!--header-->
            <div class="header_top">
                @Html.Partial("../Store/Shared/_HeaderTopPartial")
            </div>

            <div class="header-middle">
                @Html.Partial("../Store/Shared/_HeaderMiddlePartial")
            </div>

            <div class="header-bottom">
                @Html.Partial("../Store/Shared/_HeaderBottomPartial")
            </div>
        </header><!--/header-->

        @RenderBody()
    </div>
    <footer id="footer">
        <!--Footer-->

        <div class="footer-bottom">
            <div class="container">
                <div class="row">
                    <p class="pull-left">Copyright © 2013 Ze-shop Inc. Visos teisės saugomos.</p>
                    <p class="pull-right">Sukurta <span><a href="#">2FAST2FURIOUS</a></span></p>
                </div>
            </div>
        </div>

    </footer><!--/Footer-->

    @*<script src="~/Scripts/jquery.js"></script>*@
    <script src="~/libs/jquery/dist/jquery.min.js" type="text/javascript"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.scrollUp.min.js"></script>
    <script src="~/Scripts/price-range.js"></script>
    <script src="~/Scripts/jquery.prettyPhoto.js"></script>
    <script src="~/Scripts/main.js"></script>
    <script src="~/Scripts/common.js" language="javascript" type="text/javascript"></script>
    @RenderSection("scripts", required: false)
</body>