<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <title>@ViewBag.Title - FuriousWeb</title>
    @Styles.Render("~/Content/Styles/css")
    @*@Scripts.Render("~/bundles/modernizr")*@
</head>

<body>
    <div id="page">

        <div id="side-bar-left">
            @Html.Partial("_AdminLeftSideBarPartial")
        </div>

        <div class="right-side">

            <div class="top-bar">
                @Html.Partial("_AdminHeaderBarPartial")
            </div>

            <div class="main-content">
                @RenderBody() @*Home/Index*@ 
            </div>

            <div class="footer">
                <div class="copy">
                    Visos teisės saugomos &copy; @DateTime.Now.Year
                </div>
                <div class="solution">
                    Sprendimas: <span>2Fast2Furious</span>
                </div>
            </div>

        </div>

    </div>

    <script src="~/libs/jquery/dist/jquery.min.js" type="text/javascript"></script>
    <script src="~/libs/fancybox/source/jquery.fancybox.pack.js" type="text/javascript"></script>
    <script src="~/libs/TurnWheel/jReject/js/jquery.reject.js" type="text/javascript"></script>
    <script src="~/libs/Swiper/dist/js/swiper.jquery.min.js" type="text/javascript"></script>
    <script src="~/Scripts/modernizr.js" type="text/javascript"></script>
    <script src="~/Scripts/buildmobile.js" type="text/javascript"></script>
    <script src="~/Scripts/general.js" type="text/javascript"></script>
    <script src="~/Scripts/common.js" language="javascript" type="text/javascript"></script>
    @RenderSection("scripts", required: false)
</body>

</html>
